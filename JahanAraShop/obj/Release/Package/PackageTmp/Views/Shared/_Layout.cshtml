                                    <li class="mymenu">
                                        <a href="@Url.Action(MVC.Home.Index())">صفحه اصلی</a>
                                    </li>
                                    @{var Model = ExtensionMethods.MenuExtensions.menu();
                                        foreach (var First in Model.FirstLevel)
                                        {
                                            <li class="menu-item-has-children mega-menu  mymenu chech_@First.ID">
                                                <a href="#">@First.Name</a>
                                                <div class="mega-wrap">
                                                    <div class="row">
                                                        @foreach (var Second in Model.SecondLevel)
                                                        {
                                                            if (Second.ParentID == First.ID)
                                                            {
                                                                var ddd = Model.ThirdLevel.Where(x => x.ParentID == Second.ID).ToList();
                                                                if (ddd.Count != 0)
                                                                {
                                                                    <div class="col-sm-3 menu-item-has-children">
                                                                        <h6>@Second.Name</h6>
                                                                        <ul>
                                                                            @foreach (var Third in Model.ThirdLevel)
                                                                            {
                                                                                if (Third.ParentID == Second.ID)
                                                                                {
                                                                                    <li>
                                                                                        <a href="@Url.Action(MVC.Product.Categories(Third.ID))" data-id="@Third.ID">@Third.Name</a>
                                                                                    </li>
                                                                                }
                                                                            }
                                                                        </ul>
                                                                    </div>
                                                                }
                                                                if (ddd.Count == 0)
                                                                {
                                                                    break;

                                                                }
                                                            }
                                                        }
                                                    </div>
                                                </div>
                                                <ul class="sub-menu">
                                                    @foreach (var Second in Model.SecondLevel)
                                                    {
                                                        if (Second.ParentID == First.ID)
                                                        {
                                                            var ddd = Model.ThirdLevel.Where(x => x.ParentID == Second.ID).ToList();
                                                            if (ddd.Count == 0)
                                                            {
                                                                <script>
                                                                    $(".chech_@First.ID").removeClass("mega-menu");
                                                                    $(".chech_@First.ID").find(".mega-wrap").remove();
                                                                </script>

                                                                <li><a href="@Url.Action(MVC.Product.Categories(Second.ID))">@Second.Name</a></li>
                                                            }
                                                        }
                                                    }
                                                </ul>
                                            </li>
                                        }

                                    }

                                    <li class="menu-item-has-children mymenu">
                                        <a href="#">امور مشتریان</a>
                                        <ul class="sub-menu">
                                            <li><a href="@Url.Action(MVC.Home.Aboutus())">درباه ما</a></li>
