    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0' name='viewport' />
    <title>@ViewBag.Title</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <link rel="shortcut icon" type="image/png" href="~/Content/images/fav.png" />
    <!--Stylesheets -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/css/font-awesome-animation.min.css" rel="stylesheet">
    <link href="~/Content/css/pe-icon-7-stroke.css" rel="stylesheet">
    <link href="~/Content/css/icofont.css" rel="stylesheet">
    <link href="~/Content/css/animate.css" rel="stylesheet">
    <link href="~/Content/css/jquery-ui.min.css" rel="stylesheet">
    <link href="~/Content/css/slick.css" rel="stylesheet">
    <link href="~/Content/css/slick-theme.css" rel="stylesheet">
    <link href="~/Content/css/youtube-pop-up.css" rel="stylesheet">
    <link href="~/Content/css/full-screen-menu.css" rel="stylesheet">
    <link href="~/Content/css/fractionslider.css" rel="stylesheet">
    <link href="~/Content/css/component.css" rel="stylesheet">
    <link href="~/Content/css/lightcase.css" rel="stylesheet">
    <link href="~/Content/css/slider-360.css" rel="stylesheet">
    <link href="~/Content/css/amaran.min.css" rel="stylesheet">
    @*<link href="~/Content/css/style.css" rel="stylesheet" />*@
    <link href="~/Content/css/style-fa.css" rel="stylesheet" />
    <link href="~/Content/css/skin/default.css" rel="stylesheet">
    <link href="~/Content/css/responsive-fa.css" rel="stylesheet">
    <script src="~/Content/js/jquery-3.2.1.min.js"></script>
    @*<link href="~/Content/css/responsive.css" rel="stylesheet">*@
    <!--Google font-->
    <link href="https://fonts.googleapis.com/css?family=Cutive+Mono%7COpen+Sans:300,300i,400,400i,600,600i,700,700i,800%7CRaleway:300,400,400i,500,500i,600,600i,700,800" rel="stylesheet">
</head>
<body>
    <div>
        <div id="wrap">
            <header>
                <div class="top-bar bg-dark">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-6 col-sm-6 col-xs-12 site-dir hidden-xs">
                                <a href="tel:+01123456789"><i class="fa fa-phone"></i> +1 123 456 7890</a>
                                <a href="mailto:info@yourdomain.com"><i class="fa fa-envelope"></i> info@yourdomain.com</a>
                            </div>
                            <!--site dir-->
                            <div class="col-md-6 col-sm-6 col-xs-12 header-right pull-right text-right">
                                <div class="lang">
                                    <div class="dropdown">
                                        <span class="drop-down-toggle">
                                            <img src="~/Content/images/flag1.png" alt=""> Eng <i class="fa fa-angle-down"></i>
                                        </span>
                                        <ul class="drop-link trans">
                                            <li><a href="#" class="bg-dark">English</a></li>
                                            <li><a href="#" class="bg-dark">Persian</a></li>
                                        </ul>
                                    </div>
                                </div>
                                @if (Request.IsAuthenticated)
                                {
                                    <div class="account-wrap">
                                        <div class="dropdown">
                                            <span class="drop-down-toggle">
                                                <i class="fa fa-user"></i> @Html.ActionLink(MVC.Account.GetUserName(User.Identity.Name), "", "", routeValues: null, htmlAttributes: new { title = "Manage" })
                                            </span>

                                        </div>
                                    </div>
                                }
                                else
                                {
                                    <div class="account-wrap">
                                        <div class="dropdown">
                                            <span class="drop-down-toggle">
                                                <i class="fa fa-user"></i> حساب کاربری <i class="fa fa-angle-down"></i>
                                            </span>
                                            <ul class="drop-link trans">
                                                <li><a href="@Url.Action(MVC.Account.Login())" class="bg-dark">ورود</a></li>
                                                <li><a href="@Url.Action(MVC.Account.Register())" class="bg-dark">ثبت نام</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                }
                                <!--account wrap-->
                                <div class="header-cart" id="test">
                                    <a href="#" class="crt-btn drop-cart pe-7s-cart">
                                        <span class="pri-bg radius-circle pri-font BasketCount">
                                            @(HttpContext.Current.Request.Cookies.AllKeys.Contains("JahanAraShopBasketCount") ? Request.Cookies["JahanAraShopBasketCount"]?.Value : "0")
                                        </span>
                                    </a>
                                    <div class="widget widget_shopping_cart " id="SummeryCart">
                                    </div>
                                    <!--mini cart-->
                                </div>
                                @Html.Partial("~/Views/Shared/_ShowCart.cshtml")

                            </div>
                        </div>
                    </div>
                </div>
                <!--top bar-->
                <nav class="navbar navbar-default">
                    <div class="container">
                        <div class="header-mid">
                            <div class="navbar-header">
                                <a class="navbar-brand" href="index.html"><img src="~/Content/images/logo.png" alt=""></a>
                                <!--<a class="navbar-brand site-brand" href="index.html">neostore</a>-->
                                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                            </div>
                            @Html.Partial(MVC.Shared.Views._MenuPartial, Session["GoodMenu"])
                        </div>
                        <form class="search-form">
                            <input class="search-input" type="search" placeholder="جستجو" />
                        </form>
                    </div>
                </nav>
            </header>



        </div>
        @RenderBody()

    </div>
    <footer>
        <div class="footer-top bg-dark sec-padding ">
            <div class="container dir">
                <div class="row">
                    <div class="col-md-3 col-sm-3">
                        <h5>خدمات مشتریان</h5>
                        <ul class="footer-nav">
                            <li><a href="#">Contact</a></li>
                            <li><a href="#">برگشت یا تعویض کالا</a></li>
                            <li><a href="#">نقشه سایت</a></li>
                            <li><a href="#">Brand</a></li>
                            <li><a href="#">کوپن ها هدیه</a></li>
                        </ul>
                    </div>
                    <!--widget-->
                    <div class="col-md-3 col-sm-3">
                        <h5>ارتباط با جهان آرا</h5>
                        <ul class="footer-nav">
                            <li><a href="#">ارتباط با ما</a></li>
                            <li><a href="#">شرایط حمل و نقل کالا</a></li>
                            <li><a href="#">حریم خصوصی</a></li>
                            <li><a href="#">مقررات و شرایط</a></li>
                        </ul>
                    </div>
                    <!--widget-->
                    <div class="col-md-3 col-sm-3">
                        <h5>حساب کاربری من</h5>
                        <ul class="footer-nav">
                            <li><a href="#">کاربری من</a></li>
                            <li><a href="#">تاریخچه سفارشات</a></li>
                            <li><a href="#">لیست علاقه مندی</a></li>

                        </ul>
                    </div>
                    <!--widget-->
                    <div class="col-md-3 col-sm-3">
                        <h5>تماس با ما</h5>
                        <div class="contact-info">
                            <p>
                                <i class="fa fa-map-marker"></i> مشهد-الماس شرق-جهان ارا.
                            </p>
                            <p>
                                <i class="fa fa-envelope-o"></i>
                                <a href="#">info@yourdomain.com</a>
                            </p>
                            <p>
                                <i class="fa fa-phone"></i> 01 234 5678 / 01 234 5698
                            </p>
                        </div>
                    </div>
                    <!--widget-->
                </div>
            </div>
        </div>
        <div class="footer">
            <div class="container">
                <div class="row footer-bottom">
                    <div class="col-md-4 col-sm-4">
                        Copyright &copy; 2018 - Neostore, All right reserved
                    </div>
                    <div class="col-md-4 col-sm-4 cards text-center">
                        <ul class="cards">
                            <li><i class="icofont icofont-paypal"></i></li>
                            <li><i class="icofont icofont-maestro"></i></li>
                            <li><i class="icofont icofont-discover"></i></li>
                            <li><i class="icofont icofont-american-express"></i></li>
                        </ul>
                    </div>
                    <!--cards-->
                    <div class="col-md-4 col-sm-4 text-right">
                        <ul class="social">
                            <li>
                                <a href="#"><i class="fa fa-facebook"></i></a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-twitter"></i></a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-google-plus"></i></a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-instagram"></i></a>
                            </li>
                        </ul>
                    </div>
                    <!--social-->
                </div>
            </div>
        </div>
    </footer>
    <!--footer-->
    <!-- jQuery library -->

    <script src="~/Content/js/main.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/jquery-ui.min.js"></script>
    <script src="~/Content/js/touch.js"></script>
    <script src="~/Content/js/slick.min.js"></script>
    <script src="~/Content/js/jquery.countdown.min.js"></script>
    <script src="~/Content/js/count-up.min.js"></script>
    <script src="~/Content/js/jquery.waypoints.min.js"></script>
    <script src="~/Content/js/youtube-jquery.js"></script>
    <script src="~/Content/js/jquery.fractionslider.min.js"></script>
    <script src="~/Content/js/masonry.pkgd.min.js"></script>
    <script src="~/Content/js/jarallax.min.js"></script>
    <script src="~/Content/js/jarallax-video.min.js"></script>
    <script src="~/Content/js/lightcase.js"></script>
    <script src="~/Content/js/threesixty.min.js"></script>
    <script src="~/Content/js/jquery.amaran.min.js"></script>
    @RenderSection("Scripts", false);

</body>
</html>

